    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-cookie": "^1.0.6",
    "passport-local": "^1.0.0",
    "sequelize": "^4.39.0"
